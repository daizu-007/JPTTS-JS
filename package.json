{
  "name": "jptts-js",
  "version": "0.0.1",
  "description": "様々な日本語合成音声アプリを統合的に利用できるJSライブラリ",
  "module": "index.js",
  "type": "module",
  "author": "daizu-007",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daizu-007/jptts-js.git"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "start": "tsc && node dist/index.js",
    "dev": "tsc --watch",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "clean": "rm -rf dist",
    "e2e": "bun examples/example.ts"
  }
}
